
<div class="span12">
  <li id="<%= feed_item.id %>">
  <h3><span class="content"><%= feed_item.title.html_safe %></span></h3>
  <aside class="span6">
      <section>
          <span class="content"><%= feed_item.content.html_safe %></span>
          <span class="timestamp">
            Posted <%= time_ago_in_words(feed_item.created_at) %> ago.
          </span>
          <% if current_user?(feed_item.user) %>
            <%= link_to "delete", feed_item, method: :delete,
                                             data: { confirm: "You sure?" },
                                             title: feed_item.content %>
          <% end %>
        </li>
      </section>
      <section>
          <span class="content"><%= feed_item.tech_content.html_safe %></span>
      </section>
  </aside>
</div>
